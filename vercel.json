{
 "headers": [
   {
     "source": "/(.*)",
     "headers": [
       {
         "key": "X-Frame-Options",
         "value": "ALLOW-FROM https://www.notion.so"
       },
       {
         "key": "Content-Security-Policy",
         "value": "frame-ancestors 'self' https://www.notion.so"
       }
     ]
   }
 ]
}
